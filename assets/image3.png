<svg xmlns="http://www.w3.org/2000/svg" width="40" height="40" viewBox="0 0 40 40">
  <!-- Coin outer circle -->
  <circle cx="20" cy="20" r="18" fill="#FFD700"/>
  <!-- Coin inner circle -->
  <circle cx="20" cy="20" r="15" fill="#FFA500"/>
  <!-- Coin center -->
  <circle cx="20" cy="20" r="12" fill="#FFD700"/>
  <!-- Dollar sign -->
  <text x="20" y="28" font-family="Arial, sans-serif" font-size="20" font-weight="bold" fill="#FF8C00" text-anchor="middle">$</text>
  <!-- Shine effect -->
  <circle cx="14" cy="14" r="3" fill="#FFFF00" opacity="0.6"/>
</svg>
